<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TransTracker.Login"
             Title="Login">
    <VerticalStackLayout>
        <Label Text="Login"/>
        <Entry x:Name="Username" Placeholder="Enter your username"/>
        <Entry x:Name="Password" Placeholder="Enter your Password"/>
        <Button x:Name="Submit" Text="Submit" Clicked="startTracking"/>
        <Button x:Name="CreateAccount" Text="Create Account"/>
        <Label Text="Hi" x:Name="test"/>
    </VerticalStackLayout>
</ContentPage>